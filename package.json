{
  "name": "strong-json-parse",
  "version": "1.0.1",
  "description": "utils rollup scaffold",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "./dist/types/src/index.d.ts",
  "scripts": {
    "prepare": "npx husky install && npx husky add .husky/commit-msg 'npx --no -- commitlint --edit $1' && npx husky add .husky/pre-commit 'npm run eslint:fix'",
    "start": "npx rollup --config rollup.config.dev.mjs --watch",
    "dev": "npm run start",
    "build": "rm -rf lib && npx rollup --config rollup.config.build.mjs",
    "test": "npx jest",
    "eslint": "npx eslint 'src/**/*.{ts,js}'",
    "eslint:fix": "npx eslint --fix 'src/**/*.{ts,js}'"
  },
  "author": "Joey",
  "keywords": [
    "JSON",
    "bigint",
    "parse",
    "json",
    "stringify",
    "json schema"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.3",
    "rollup": "^3.3.0",
    "rollup-plugin-serve": "^2.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run eslint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "registry": "https://registry.npmjs.org/",
  "repository": "https://github.com/chengandguo/strong-json-parse"
}
